const user = {
                id: 101,
                name: "Ravi",
                preferences: {
                  theme: "dark",
                  language: "en"
                }
              };
const copyUser = structuredClone(user)
copyUser.name = "Pavan"
copyUser.preferences.theme = "white"
console.log(user)
console.log(copyUser)

