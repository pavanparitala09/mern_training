//ASSIGNMENT 2 
const order = {
                  orderId: "ORD1001",
                  customer: {
                    name: "Anita",
                    address: {
                      city: "Hyderabad",
                      pincode: 500085
                    }
                  },
                  items: [
                    { product: "Laptop", price: 70000 }
                  ]
                };
const copyOrder = structuredClone(order)
copyOrder.customer.address.city = "madhira";
console.log(order)
console.log(copyOrder)
